Algoritmo Ej14
	Definir num1, num2 como reales;
	
	Escribir "Introduzca un número";
	Leer num1;
	
	Escribir "Introduzca otro número";
	Leer num2;
	
	Escribir "Suma: " num1 + num2;
	Escribir "Resta: " num1 - num2;
	Escribir "Producto: " num1 * num2;
	Escribir "División: " num1 / num2;
	

FinAlgoritmo
